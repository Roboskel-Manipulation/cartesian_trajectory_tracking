<?xml version="1.0"?>
<launch>

	<arg name="trajectory_file" default="/home/ttsitos/Desktop/yaml_files/fast_gpu/cloudy_day/smooth_yaml/md_arrow_n.yaml"/>
	<arg name="case_piecewise" default="false"/>
	<arg name="case_bezier" default="false"/>
	<arg name="case_downsampling" default="false"/>	

	<group if="$(arg case_piecewise)">
		 <node name="trajectory_read" type="raw_read.py" pkg="reactive_control" args="$(arg trajectory_file)" respawn="false" output="log">
		</node>
		

		<!-- <node name="trajectory_read" type="processed_points_read.py" pkg="reactive_control" args="$(arg trajectory_file)" respawn="false" output="log">
		</node>
 -->		
		 <node name="piecewise_bezier_constructor" type="reactive_piecewise_bezier.py" pkg="reactive_control" respawn="false" output="screen">
		</node> 
		
		
		<include file="$(find trajectory_process_utils)/launch/trajectory_smoothing.launch">
		</include>
	</group>

	<group if="$(arg case_bezier)">
		<node name="bezier_read" type="smooth_read.py" pkg="reactive_control" respawn="false" output="log" args="$(arg trajectory_file)">
		</node>
	</group>

	<group if="$(arg case_downsampling)">
		<node name="trajectory_read" type="raw_read.py" pkg="reactive_control" args="$(arg trajectory_file)" respawn="false" output="log">
		</node>
		
		<!-- <node name="trajectory_read" type="processed_points_read.py" pkg="reactive_control" args="$(arg trajectory_file)" respawn="false" output="log">
		</node> -->
		
		<node name="downsampling_interpolation_constructor" type="reactive_std_downsampling.py" pkg="reactive_control" respawn="false" output="screen">
		</node>
	</group>


	<include file="$(find reactive_control)/launch/reactive_control_launch.launch">
	</include>

	<include file="$(find manos_gazebo)/launch/manos_gazebo.launch">
		<arg name="cart_control" value="true"/>
	</include>
</launch>
