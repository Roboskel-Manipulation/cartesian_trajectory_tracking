<?xml version="2.0" ?>
<launch>
	<arg name="trajectory_file" default="/home/ttsitos/check_input.yaml"/>
	<arg name="camera" default="camera" />

	<node pkg="tf" type="static_transform_publisher" name="dummy_tf_publisher"
        args="-0.118 1.156 0.625 3.112 -0.001 -1.930 /base_link /camera_rgb_optical_frame 100">
  	</node>
	
    <include file="$(find frame_transpose)/launch/frame_transpose.launch">
    </include>

    <node pkg="reactive_control" type="camera_points_read.py" name="camera_points_read" output="screen" respawn="false" args="$(arg trajectory_file)">
    </node>

</launch>


